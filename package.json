{
  "name": "kdupon.github.io",
  "version": "0.1.0",
  "description": "Kate Dupon",
  "main": "index.js",
  "scripts": {
    "build": "bundle exec jekyll build && webpack --progress --config webpack/webpack.config.js",
    "build:jekyll": "bundle exec jekyll build",
    "build:webpack": "webpack --progress --config webpack/webpack.config.js",
    "lint": "npm-run-all --parallel lint:*",
    "lint:markup": "echo \"Error: no test specified\" && exit 1",
    "lint:scripts": "echo \"Error: no test specified\" && exit 1",
    "lint:styles": "echo \"Error: no test specified\" && exit 1",
    "package": "npm-run-all --serial package:webpack package:jekyll",
    "package:jekyll": "npm run build:jekyll",
    "package:webpack": "webpack --progress -p --config webpack/webpack.config.js",
    "start": "npm-run-all --parallel start:*",
    "start:jekyll": "bundle exec jekyll serve",
    "start:webpack": "webpack --hide-modules --watch --config webpack/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdupon/kdupon.github.io.git"
  },
  "keywords": [
    "Kate",
    "Dupon"
  ],
  "author": "Joshua L Geschwendt",
  "contributors": [
    "Kate Dupon"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kdupon/kdupon.github.io/issues"
  },
  "homepage": "https://github.com/kdupon/kdupon.github.io#readme",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-sync": "^2.18.6",
    "browsersync-webpack-plugin": "^0.3.3",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.1",
    "postcss-loader": "1.2.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-merge": "^2.4.0"
  }
}
