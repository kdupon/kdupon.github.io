{
  "name": "kdupon.github.io",
  "version": "0.1.0",
  "description": "Kate Dupon",
  "main": "index.js",
  "scripts": {
    "bsync": "browser-sync start --config _theme/webpack/bsync/config.js",
    "build": "npm-run-all build:webpack build:jekyll",
    "build:jekyll": "JEKYLL_ENV=development bundle exec jekyll build",
    "build:webpack": "webpack --progress --config _theme/webpack/webpack.config.js",
    "release": ". ./_bin/release.sh",
    "david": "david",
    "deploy": ". ./_bin/deploy.sh",
    "docker": "npm-run-all docker:start docker:attach",
    "docker:attach": "docker exec -it katedupon.com /bin/bash",
    "docker:build": "docker build -t katedupon/jekyll .",
    "docker:create": "docker create --name katedupon.com -p 3000:3000 -p 3001:3001 -v $(pwd):/var/www katedupon/jekyll",
    "docker:start": "docker start katedupon.com",
    "docker:stop": "docker stop katedupon.com",
    "help": "npm run info",
    "info": "npm-scripts-info",
    "lint": "npm-run-all --parallel lint:*",
    "lint:scripts": "eslint _theme",
    "lint:styles": "scss-lint",
    "package": "npm-run-all package:webpack package:jekyll",
    "package:jekyll": "JEKYLL_ENV=production bundle exec jekyll build",
    "package:webpack": "webpack --progress -p --config _theme/webpack/webpack.config.js",
    "profile": "webpack --progress --profile --json --config _theme/webpack/webpack.config.js",
    "start": "npm-run-all --parallel serve:*",
    "start:dev": "npm-run-all build bsync",
    "start:prod": "npm-run-all package bsync",
    "serve:jekyll": "JEKYLL_ENV=server bundle exec jekyll serve",
    "serve:webpack": "webpack --hide-modules --watch --config _theme/webpack/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdupon/kdupon.github.io.git"
  },
  "keywords": [
    "Kate",
    "Dupon"
  ],
  "author": "Joshua L Geschwendt",
  "contributors": [
    "Kate Dupon"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kdupon/kdupon.github.io/issues"
  },
  "homepage": "https://github.com/kdupon/kdupon.github.io#readme",
  "dependencies": {
    "bootstrap": "twbs/bootstrap#78fc4d23fcefe31397444b75f43728c38e13c590",
    "imagesloaded": "^4.1.1",
    "masonry-layout": "^4.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "browser-sync": "^2.18.8",
    "browsersync-webpack-plugin": "^0.4.1",
    "bs-html-injector": "^3.0.3",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.27.3",
    "david": "^11.0.0",
    "eslint": "^3.18.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "minimist": "^1.2.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.0.2",
    "npm-scripts-info": "^0.3.6",
    "postcss-loader": "1.3.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.15.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0"
  }
}
